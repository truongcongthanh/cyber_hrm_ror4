<% @vungdialies.each do |vungdialy| %>
    <tr>
      <td><%= vungdialy.ma %></td>
      <td><%= vungdialy.ten %></td>
      <td><%= link_to edit_vungdialy_path(vungdialy), data: {remote: true}, :title => t('edit') do %>
            <i class="glyphicon glyphicon-edit"></i>
        <% end %>
        <%= link_to vungdialy_delete_path(vungdialy), data: {remote: true}, :title => t('delete') do %>
            <i class="glyphicon glyphicon-trash"></i>
        <% end %>
      </td>
    </tr>
<% end %>