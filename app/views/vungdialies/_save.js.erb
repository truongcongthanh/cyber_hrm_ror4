$("ul.errors").html("")
<% if @vungdialy.errors.any? %>
<% @vungdialy.errors.full_messages.each do |message| %>
$("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
<% end %>
<% else %>
$(".vungdialy-index").html("<%= escape_javascript(render 'index') %>")
$("#vungdialy-modal").modal("hide")
<% end %>