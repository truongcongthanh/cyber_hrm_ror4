  <%= link_to new_nhombangcap_path, remote: true, class: "btn btn-primary pull-right" do %>
      <i class="glyphicon glyphicon-plus-sign"></i> <%= t('new') %>
  <% end %>
  <div class="clearfix"></div>
</div>
</div>
<div class="new-form"></div>

<table class="table table-hover ta">
  <thead>
  <tr>
    <th style="width: 150px;"><%= t "nhombangcaps.ma" %></th>
    <th style="width: 230px;"><%= t "nhombangcaps.ten" %></th>
    <th><%= t "nhombangcaps.diengiai" %></th>
    <td style="width: 60px;">&nbsp;</td>
  </tr>
  </thead>
  <tbody class="nhombangcap-index">
  <%= render "index" %>
  </tbody>
</table>
<div id="form-modal" class="modal fade">
  <div class="modal-dialog">
    <div id="inner-form-modal" class="modal-content"></div>
  </div>
</div>