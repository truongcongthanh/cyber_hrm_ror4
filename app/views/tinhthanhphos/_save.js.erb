$("ul.errors").html("")
<% if @tinhthanhpho.errors.any? %>
<% @tinhthanhpho.errors.full_messages.each do |message| %>
$("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
<% end %>
<% else %>
$(".tinhthanhpho-index").html("<%= escape_javascript(render 'index') %>")
$("#tinhthanhpho-modal").modal("hide")
<% end %>