$("ul.errors").html("")
<% if @phuongxa.errors.any? %>
<% @phuongxa.errors.full_messages.each do |message| %>
$("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
<% end %>
<% else %>
$(".form-index").html("<%= escape_javascript(render 'index') %>")
$("#form-modal").modal("hide")
<% end %>