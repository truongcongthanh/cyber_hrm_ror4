<% @quoctiches.each do |quoctich| %>
    <tr>
      <td><%= quoctich.ma %></td>
      <td><%= quoctich.ten %></td>
      <td><%= link_to edit_danhmuc_quoctich_path(quoctich), data: {remote: true}, :title => t('edit') do %>
            <i class="glyphicon glyphicon-edit"></i>
        <% end %>
        <%= link_to danhmuc_quoctich_delete_path(quoctich), data: {remote: true}, :title => t('delete') do %>
            <i class="glyphicon glyphicon-trash"></i>
        <% end %>
      </td>
    </tr>
<% end %>