            options_thuongtru_huyen = $(thuongtru_huyen).filter("optgroup[label='" + escaped_thuongtru_tinh + "']").html();
            if (options_thuongtru_huyen)
                $('#nhanvien_thuongtru_huyen').html(options_thuongtru_huyen);
            else
