            options_nguyenquan_xa = $(nguyenquan_xa).filter("optgroup[label='" + escaped_nguyenquan_huyen + "']").html();
            if (options_nguyenquan_xa)
                $('#nhanvien_nguyenquan_xa').html(options_nguyenquan_xa);
            else
                $('#nhanvien_nguyenquan_xa').empty();
        });

        var thuongtru_huyen = $('#nhanvien_thuongtru_huyen').html();
        $('#nhanvien_thuongtru_tinh').change( function () {
            var escaped_thuongtru_tinh, thuongtru_tinh, options_thuongtru_huyen;
            thuongtru_tinh = $('#nhanvien_thuongtru_tinh :selected').text();
