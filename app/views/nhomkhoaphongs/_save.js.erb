$("ul.errors").html("")
<% if @nhomkhoaphong.errors.any? %>
<% @nhomkhoaphong.errors.full_messages.each do |message| %>
$("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
<% end %>
<% else %>
$(".nhomkhoaphong-index").html("<%= escape_javascript(render 'index') %>")
$("#nhomkhoaphong-modal").modal("hide")
<% end %>